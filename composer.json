{
    "name": "sonata-project/doctrine-phpcr-admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata / Integrate Doctrine PHPCR into the SonataAdminBundle",
    "keywords": [
        "admin",
        "admin-generator",
        "bootstrap",
        "sonata",
        "symfony-cmf"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nacho Martin",
            "email": "nacho.martin@gmail.com",
            "homepage": "http://nacho-martin.com"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDoctrinePHPCRAdminBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "doctrine/phpcr-odm": "^1.5.2 || ^2.0",
        "sonata-project/admin-bundle": "^3.63",
        "sonata-project/block-bundle": "^3.18.3",
        "symfony-cmf/resource-rest-bundle": "^1.0.1",
        "symfony-cmf/tree-browser-bundle": "^2.0",
        "symfony/config": "^3.4.36 || ^4.4",
        "symfony/dependency-injection": "^3.4.36 || ^4.4",
        "symfony/finder": "^3.4.36 || ^4.4",
        "symfony/form": "^3.4.36 || ^4.4",
        "symfony/framework-bundle": "^3.4.36 || ^4.4",
        "symfony/http-foundation": "^3.4.36 || ^4.4",
        "symfony/http-kernel": "^3.4.36 || ^4.4",
        "symfony/options-resolver": "^3.4.36 || ^4.4",
        "symfony/property-access": "^3.4.36 || ^4.4"
    },
    "conflict": {
        "jms/serializer-bundle": "<2.3.1",
        "sonata-project/doctrine-extensions": "<=1.3.0"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "require-dev": {
        "dantleech/glob-finder": "^1.0",
        "doctrine/cache": "^1.0",
        "doctrine/doctrine-bundle": "^1.12.6 || ^2.0",
        "jackalope/jackalope-doctrine-dbal": "^1.4.1",
        "jackalope/jackalope-jackrabbit": "^1.3.2",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpcr/phpcr": "^2.1",
        "phpcr/phpcr-utils": "^1.4.0",
        "phpunit/phpunit": "^9.5",
        "sonata-project/core-bundle": "^3.14",
        "symfony-cmf/resource": "^1.0",
        "symfony-cmf/resource-bundle": "^1.0",
        "symfony-cmf/testing": "^3.0.0",
        "symfony/css-selector": "^3.4.22 || ^4.4 || ^5.3",
        "symfony/doctrine-bridge": "^3.4.22 || ^4.4 || ^5.3",
        "symfony/monolog-bundle": "^3.5",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/routing": "^3.4.36 || ^4.4",
        "symfony/security-acl": "^2.8 || ^3.0",
        "symfony/validator": "^3.4 || ^4.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sonata\\DoctrinePHPCRAdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\DoctrinePHPCRAdminBundle\\Tests\\": "tests/"
        }
    },
    "abandoned": true
}
